{
  "keywords": [
    "svelte"
  ],
  "dependencies": {
    "svelte-common": "^1.3.1",
    "svelte-guard-history-router": "^1.1.16",
    "svelte-session-manager": "^1.0.7"
  },
  "devDependencies": {
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-dev": "^1.1.1",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "svelte": "^3.19.2"
  },
  "scripts": {
    "prepare": "rollup -c",
    "start": "rollup -c -w"
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-web-app",
      "arlac77/template-rollup"
    ],
    "usedBy": [
      "arlac77/entitlement-provider-frontend",
      "arlac77/hook-ci-frontend-svelte",
      "arlac77/konsum-frontend-svelte",
      "arlac77/system-dashboard-frontend"
    ],
    "mergers": [
      {
        "type": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "Svelte v3",
              "icon": "https://img.shields.io/badge/svelte-v3-orange.svg",
              "url": "https://svelte.dev",
              "order": 0.05
            }
          ]
        }
      }
    ]
  }
}
