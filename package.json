{
  "keywords": [
    "svelte"
  ],
  "dependencies": {
    "svelte-common": "^2.0.3",
    "svelte-guard-history-router": "^1.1.18",
    "svelte-session-manager": "^1.0.11"
  },
  "devDependencies": {
    "rollup-plugin-postcss": "^3.1.1",
    "postcss-import": "^12.0.1",
    "rollup-plugin-svelte": "^5.2.1",
    "svelte": "^3.22.2"
  },
  "scripts": {
    "prepare": "rollup -c",
    "start": "rollup -c -w"
  },
  "config": {
    "api": "/services/{{name}}/api",
    "proxyTarget": "https://somewhere/"
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-web-app",
      "arlac77/template-rollup"
    ],
    "usedBy": [
      "arlac77/SymatemExplorer",
      "arlac77/entitlement-provider-frontend",
      "arlac77/hook-ci-frontend-svelte",
      "arlac77/konsum-frontend-svelte",
      "arlac77/symatemexplorer",
      "arlac77/system-dashboard-frontend"
    ],
    "mergers": [
      {
        "type": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "Svelte v3",
              "icon": "https://img.shields.io/badge/svelte-v3-orange.svg",
              "url": "https://svelte.dev",
              "order": 0.05
            }
          ]
        }
      }
    ]
  }
}
